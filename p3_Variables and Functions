import random

def get_choices():
  player_choice = input ("Enter a choice [rock,paper,scissors]: ")
  options = ["rock","paper","scissorsscis"]
  computer_choice = random.choice(options)
  choices = {
            "player": player_choice,
            "computer": computer_choice
            }
  return choices

choices = get_choices()
print(choices)
dict = {
        "name" : "Kaushik",
        "color" : "choices"
        }

